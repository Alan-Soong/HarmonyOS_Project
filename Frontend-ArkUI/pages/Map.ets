  // 地图配置
  private mapOption: mapCommon.MapOptions = {
    position: {
      target: { latitude: 39.1372, longitude: 117.1960 },
      zoom: this.currentZoom
    }
  };
  // 地图初始化回调
  private readonly callback =
    (err: BusinessError | null, controller: map.MapComponentController): void => {
      if (err) {
        hilog.error(0xFF00, 'MapInit', '初始化失败: %{public}s', JSON.stringify(err));
        return;
      }
      this.mapController = controller;
      this.setupMarkerInteractions();
      this.searchStores(); // 初始加载全部标记
    };

  // 设置标记交互
  private setupMarkerInteractions(): void {
    this.mapController?.on('markerClick', (marker: map.Marker) => {
      this.showStoreInfo(marker.getTitle());
    });
  }

  // 显示餐厅信息
  private showStoreInfo(storeName: string): void {
    const store = this.restaurants.find(s => s.name === storeName);
    if (store) {
      prompt.showDialog({
        title: store.name,
        message: `${store.district}\n${store.info}`,
        buttons: [{ text: '确定', color: '#007DFF' }]
      }).then(() => {
        this.highlightStore(store);
      });
    }
  }

  // 高亮显示餐厅
  private async highlightStore(store: LocationInfo): Promise<void> {
    if (!this.mapController) return;

    try {
      // 清除旧高亮标记
      if (this.highlightMarker) {
        await this.highlightMarker.remove();
      }

      // 添加新高亮标记（保持可点击）
      this.highlightMarker = await this.mapController.addMarker({
        position: { latitude: store.lat, longitude: store.lng },
        icon: $r('app.media.food'),
        title: store.name, // 保持原始名称，不要加"(当前选择)"
        snippet: store.info,
        clickable: true, // 确保可点击
        zIndex: 999
      });

      // 动画跳转
      this.mapController.animateCamera({
        target: { latitude: store.lat, longitude: store.lng },
        zoom: 16
      }, 500);

    } catch (error) {
      hilog.error(0xFF00, 'Highlight', '标记高亮失败: %{public}s', JSON.stringify(error));
    }
  }

  // 添加标记
  private async addMarkers(stores: LocationInfo[]): Promise<void> {
    if (!this.mapController) return;

    try {
      // 清除旧标记
      await this.clearMarkers();

      // 添加新标记
      this.markers = await Promise.all(
        stores.map(async (store): Promise<map.Marker> => {
          return await this.mapController!.addMarker({
            position: { latitude: store.lat, longitude: store.lng },
            icon: $r('app.media.icon_chooseplace'),
            title: store.name,
            snippet: "点击查看详情",
            clickable: true
          });
        })
      );
    } catch (error) {
      hilog.error(0xFF00, 'AddMarkers', '标记添加失败: %{public}s', JSON.stringify(error));
    }
  }

  // 清除标记
  private async clearMarkers(): Promise<void> {
    await Promise.all(this.markers.map(m => m.remove()));
    this.markers = [];
  }

  // 搜索餐厅
  private searchStores(): void {
    let filteredStores = this.restaurants;

    // 按区域筛选
    if (this.selectedDistrict !== '全部') {
      filteredStores = filteredStores.filter(store =>
      store.district === this.selectedDistrict
      );
    }

    // 按关键词筛选
    if (this.searchKeyword) {
      filteredStores = filteredStores.filter(store =>
      store.name.includes(this.searchKeyword) ||
      store.info.includes(this.searchKeyword)
      );
    }

    this.addMarkers(filteredStores);
  }

  build() {
    Column() {
      // 搜索栏与搜索按钮一行 - 整体背景改为橙色
      Row() {
        // 搜索输入框
        TextInput({ placeholder: '搜索餐厅名/地址' })
          .width('65%')
          .height(40)
          .margin({ right: 8 })
          .borderRadius(20)
          .backgroundColor('#FFFFFF')
          .onChange(value => this.searchKeyword = value)

        // 区域选择按钮
        Button(this.selectedDistrict)
          .width('20%')
          .height(40)
          .margin({ right: 8 })
          .borderRadius(20)
          .backgroundColor('#FFFFFF')
          .fontColor('#333333')
          .onClick(() => this.pickerVisible = true)

        // 搜索图标按钮 - 背景改为白色
        Button() {
          Image($r('app.media.icon_search')) // 搜索图标
            .width(20)
            .height(20)
        }
        .width(40)
        .height(40)
        .borderRadius(20)
        .backgroundColor('#FFFFFF') // 改为白色背景
        .onClick(() => this.searchStores())
      }
      .width('95%')
      .padding(10)
      .backgroundColor('#FFA500') // 搜索栏整体背景改为橙色
      .borderRadius(20)
      .margin({ top: 10 })

      // 地图组件
      MapComponent({
        mapOptions: this.mapOption,
        mapCallback: this.callback
      })
        .width('100%')
        .height('85%')


      // 行政区选择弹窗
      if (this.pickerVisible) {
        Column() {
          // 标题栏
          Row() {
            Text('选择行政区')
              .fontSize(18)
              .fontWeight(FontWeight.Bold)
              .margin({ left: 12 })

            Blank()

            Button() {
              Image($r('app.media.icon_error')) // 关闭图标
                .width(16)
                .height(16)
            }
            .width(32)
            .height(32)
            .margin({ right: 8 })
            .backgroundColor(Color.Transparent) // 透明背景
            .onClick(() => this.pickerVisible = false)
          }
          .width('100%')
          .margin({ bottom: 12 })

          // 滚动选择器
          Scroll() {
            Column() {
              ForEach(this.districts, (district: string) => {
                Button(district)
                  .width('90%')
                  .height(48)
                  .margin({ bottom: 8 })
                  .backgroundColor(district === this.selectedDistrict ? '#007DFF' : '#F5F5F5')
                  .fontColor(district === this.selectedDistrict ? '#FFFFFF' : '#333333')
                  .fontSize(16)
                  .borderRadius(8)
                  .onClick(() => {
                    this.selectedDistrict = district;
                    this.pickerVisible = false;
                    this.searchStores();
                  })
              })
            }
            .width('100%')
          }
          .height(300)
          .width('100%')
          .scrollable(ScrollDirection.Vertical)
          .scrollBar(BarState.On)
        }
        .width('90%')
        .height('50%')
        .padding(12)
        .backgroundColor('#FFFFFF')
        .borderRadius(16)
        .shadow(10)
        .position({ x: '5%', y: '10%' })
      }
    }
    .width('100%')
    .height('100%')
    .backgroundColor('#F5F5F5')
  }
}

